        const response = await apiClient.conductHypothesisInterview(i, extractedQuestions);
        results[response.persona_name] = response.interview_results;
        
        const completedProgress = Math.round(((i + 1) / totalPersonas) * 100);
        setProgress(completedProgress);
        setProgressMessage(`ãƒšãƒ«ã‚½ãƒŠ ${i + 1}/${totalPersonas} ã®ä»®èª¬æ¤œè¨¼å®Œäº†`);
      }
      
      setProgress(100);
      setProgressMessage('ä»®èª¬æ¤œè¨¼ã‚¤ãƒ³ã‚¿ãƒ“ãƒ¥ãƒ¼å®Œäº†');
      setHypothesisInterviewResults(results);
      setStep(7);
    } catch (err: any) {
      setError('ä»®èª¬æ¤œè¨¼ã‚¤ãƒ³ã‚¿ãƒ“ãƒ¥ãƒ¼ã®å®Ÿè¡Œã«å¤±æ•—ã—ã¾ã—ãŸ: ' + (err.response?.data?.detail || err.message));
